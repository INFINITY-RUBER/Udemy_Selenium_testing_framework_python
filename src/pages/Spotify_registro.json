{
  "Logo": {
    "GetFieldBy": "Xpath",
    "ValueToFind": "//svg[contains(@role,'img')]"
  },

  "Titulo": {
    "GetFieldBy": "Xpath",
    "ValueToFind": "//h2[contains(text(),'Registrarte con tu correo electrónico')]"
  },

  "Email": {
    "GetFieldBy": "Xpath",
    "ValueToFind": "//input[@id='email']"
  },

  "Email Confirmacion": {
    "GetFieldBy": "Xpath",
    "ValueToFind": "//input[@id='confirm']"
  },

  "Nombre": {
    "GetFieldBy": "Xpath",
    "ValueToFind": "//input[@id='displayname']"
  },

  "Pass": {
    "GetFieldBy": "id",
    "ValueToFind": "password"
  },

  "Mes de nacimiento": {
    "GetFieldBy": "Xpath",
    "ValueToFind": "//select[@id='month']"
  },

  "Email Error": {
    "GetFieldBy": "Xpath",
    "ValueToFind": "//span[contains(text(),'Este correo electrónico no es válido. Asegúrate de')]"
  }
}
