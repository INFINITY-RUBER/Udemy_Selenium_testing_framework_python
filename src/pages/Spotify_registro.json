{
  "Logo": {
    "GetFieldBy": "Xpath",
    "ValueToFind": "//svg[contains(@role,'img')]"
  },

  "Titulo": {
    "GetFieldBy": "Xpath",
    "ValueToFind": "/html[1]/body[1]/div[1]/main[1]/div[2]/div[1]/form[1]/h2[1]"
  },

  "Already": {
    "GetFieldBy": "Xpath",
    "ValueToFind": "/html[1]/body[1]/div[1]/main[1]/div[2]/div[1]/form[1]/div[9]/p[3]"
  },

  "Email": {
    "GetFieldBy": "Xpath",
    "ValueToFind": "//input[@id='email']"
  },

  "Email Confirmacion": {
    "GetFieldBy": "Xpath",
    "ValueToFind": "//input[@id='confirm']"
  },

  "Nombre": {
    "GetFieldBy": "Xpath",
    "ValueToFind": "//input[@id='displayname']"
  },

  "Pass": {
    "GetFieldBy": "id",
    "ValueToFind": "password"
  },
  "Dia":{
    "GetFieldBy": "Xpath",
    "ValueToFind": "//*[@id='day']"
  },

  "Mes de nacimiento": {
    "GetFieldBy": "Xpath",
    "ValueToFind": "//select[@id='month']"
  },
  "Año": {
    "GetFieldBy": "Xpath",
    "ValueToFind": "//*[@id='year']"
  },

  "Email Error": {
    "GetFieldBy": "Xpath",
    "ValueToFind": "//span[contains(text(),'Este correo electrónico no es válido. Asegúrate de')]"
  },

  "Contenedor Captcha": {
    "GetFieldBy": "Xpath",
    "ValueToFind": "//select[@id='month']"
  },

  "Mensaje Email": {
    "GetFieldBy": "Xpath",
    "ValueToFind": "//body/div[@id='__next']/main[1]/div[2]/form[1]/div[1]/div[2]/span[1]"
  }
}
